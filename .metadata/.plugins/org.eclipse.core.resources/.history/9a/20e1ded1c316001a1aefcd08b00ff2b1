			String Text = rs.getString(1);				
		//	while(rs.next()) 
			
				
				System.out.println(Text);
			//System.out.println(rs.getInt(1)+"  "+rs.getString(2)+"  "+rs.getString(3)+"  "+rs.getString(4)+"  "+rs.getInt(5)+"  "+rs.getString(6));

			con.close();  
			}catch(Exception e){ System.out.println(e);}  
			}  
}
